{
  "env": {
    "browser": true,
    "es2020": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "ecmaVersion": 8
    // "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-unused-vars": 1,
    "jsx-a11y/label-has-for": 0,
    "react/prop-types": 0
  }
}
